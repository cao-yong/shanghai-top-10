    display: flex;
    align-items: center;
    padding: 0 20rpx 30rpx;
}

.search-view {
    flex-grow: 1;
    position: relative;
    margin: 0 16rpx;
}

.search-view::before {
    position: absolute;
    left: 20rpx;
    top: 50%;
    content: "";
    height: 30rpx;
    width: 30rpx;
    transform: translateY(-50%);
    background: url(data:image/png;base64,) center center / 26rpx 26rpx no-repeat;
    z-index: 2;
}

.m-city-list {
    width: 100%;
    padding: 0 60rpx 0 30rpx;
    padding-top: 0!important;
    background-color: #FFFFFF;
}

.m-city-list .m-city-list-li,
.m-city-list-li {
    height: 88rpx;
    line-height: 84rpx;
    background-color: #fff;
    border-bottom: 1px solid #E4E4E4;
    -webkit-tap-highlight-color: transparent;
    font-size: 28rpx;
    color: #333333
}

.m-city-list-search-li {
    padding-left: 20rpx;
}

.g-letter-list {
    list-style: none;
    /* background-color: #fff; */
    position: absolute;
    top: 200rpx;
    bottom: 0;
    right: 0;
    width: 58rpx;
    overflow: hidden;
    -webkit-user-select: none;
    user-select: none;
}

.g-letter-list>.g-letter-list-li {
    text-align: center;
    height: 100%;
}

.g-letter-list .g-letter-list-li .g-letter-list-li-a {
    display: block;
    height: 3.8%;
    text-decoration: none;
    color: #666666;
    font-size: 20rpx;
    line-height: 40rpx;
    -webkit-tap-highlight-color: transparent;
    text-align: center;
}

.m-city-list .m-city-list-li:last-child {
    border-bottom: none;
}


