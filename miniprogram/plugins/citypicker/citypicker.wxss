.m-city-scr-cur,
.m-city-scr-hot,
.m-city-scr-hty {
    padding: 0 20rpx;
    /* background-color: #fff; */
    overflow: hidden;
}

.m-city-scr-hot view.m-hot-city,
.m-city-scr-hty view.m-hty-city {
    float: left;
    width: 21.5%;
    max-width: 240rpx;
    margin: 10rpx;
    text-align: center;
    -webkit-tap-highlight-color: transparent;
    background: #FFFFFF;
    border-radius: 10rpx;
}

.m-city-scr-cur view.m-cur-city {
    float: left;
    width: 21.5%;
    max-width: 240rpx;
    margin: 10rpx;
    -webkit-tap-highlight-color: transparent;
    /* background: #F5F5F5; */
    background: #FFFFFF;
    border-radius: 10rpx;
}

.m-city-scr-cur view.m-cur-city view,
.m-city-scr-hot view.m-hot-city view,
.m-city-scr-hty view.m-hty-city view {
    display: block;
    width: 148rpx;
    height: 64rpx;
    line-height: 64rpx;
    /* border: 1rpx solid #ddd; */
    -webkit-tap-highlight-color: #ddd;
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
    font-size: 26rpx;
    color: #333333;
    border-radius: 6rpx;
}

.city-list .u-city-sct-hty-hd,
.city-list .u-city-sct-cur-hd,
.city-list .u-city-sct-hot-hd,
.sc-letter-anchor {
    font-size: 24rpx;
    padding-left: 20rpx;
}

