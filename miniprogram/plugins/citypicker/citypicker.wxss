#letterTip {
    position: relative;
    margin: 50% auto;
    color: #fff;
    text-align: center;
    border-radius: 50%;
    z-index: 4;
    background-color: #4287FF;
    opacity: 0;
    display: none;
}

#letterTip #letterTip_word {
    text-align: center;
}

#letterTip,
.letterTipShow {
    width: 168rpx;
    height: 168rpx;
    line-height: 168rpx;
    font-size: 64rpx;
}

.letterTipHide {
    transition: 1s;
    width: 80rpx;
    height: 80rpx;
    line-height: 80rpx;
    font-size: 40rpx;
}

.letterTipShow {
    transition: 1s;
    display: block!important;
    opacity: 1 !important;
    width: 168rpx;
    height: 168rpx;
    line-height: 168rpx;
    font-size: 64rpx;
}

.searchBtn {
    height: 60rpx;
    width: 60rpx;
    background-image: url(http://pic.lvmama.com/mobile/img/sp/smart/fangdajing.png);
    background-size: 30rpx;
    background-position: 20rpx center;
    background-repeat: no-repeat;
}

