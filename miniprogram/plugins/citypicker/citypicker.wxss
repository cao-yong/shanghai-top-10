/**
 * iPhone 4/4s portrait
 */

@media only screen and (min-device-width: 320rpx) and (max-device-width: 480rpx) and (-webkit-device-pixel-ratio: 2) and (device-aspect-ratio: 2/3) and (orientation:portrait) {
    .city-list {
        height: 480rpx;
    }
}


/**
 * iPhone 4/4s landscape
 */

@media only screen and (min-device-width: 320rpx) and (max-device-width: 480rpx) and (-webkit-device-pixel-ratio: 2) and (device-aspect-ratio: 2/3) and (orientation:landscape) {
    .city-list {
        height: 480rpx;
    }
}


/**
 *  iPhone 5/5s landscape & portrait
 */

@media only screen and (min-device-width: 320rpx) and (max-device-width: 568rpx) and (-webkit-min-device-pixel-ratio: 2) {
    .city-list {
        height: 568rpx;
    }
}


/**
 *  iPhone 5/5s landscape & portrait
 */

@media only screen and (min-device-width: 414rpx) and (max-device-width: 736rpx) and (-webkit-min-device-pixel-ratio: 3) {
    .city-list {
        height: 736rpx;
    }
}

@media only screen and (min-device-width: 375rpx) and (max-device-width: 667rpx) and (orientation: portrait) {
    .city-list {
        height: 667rpx;
    }
}

@media only screen and (min-device-width: 375rpx) and (max-device-width: 667rpx) and (orientation: landscape) {
    .city-list {
        height: 667rpx;
    }
}

@media only screen and (min-device-width: 414rpx) and (max-device-width: 736rpx) and (orientation: portrait) {
    .city-list {
        height: 736rpx;
    }
}

@media only screen and (min-device-width: 414rpx) and (max-device-width: 736rpx) and (orientation: landscape) {
    .city-list {
        height: 736rpx;
    }
}

