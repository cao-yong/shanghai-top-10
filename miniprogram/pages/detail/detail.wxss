  text-align: justify;
  line-height: 30rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  margin-bottom: 20rpx;
  font-weight: bold;
}

.note-swiper{
  width: calc(80% + 30rpx) !important;
  /* padding-left: 30rpx; */
