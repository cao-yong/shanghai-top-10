/* 添加行程弹框 */
.no-scroll{
   width: 100%;
   height: 100%;
   position: fixed;
   top: 0;
   left: 0;
   overflow: hidden;
   z-index: 0;
}
.mask-layer{
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    left: 0;
    z-index: 120;
    background-color: rgba(0, 0, 0, .5);
}

.content-layer{
  position: fixed;
  bottom: 0;
  width: 100%;
  height: auto;
  background: #fff;
  z-index: 150;
}

.content-head, 
.content-theme{
  width: 100%;
  height: 46px;
  line-height: 46px;
  text-align:  center;
  background: #FFFFFF;
  font-family: PingFangSC-Regular;
  font-size: 30rpx;
  color: #666666;
  border-bottom: 1px solid#E8E8E8;
}
.content-theme{
  color: #000000;
}
.footer {
    /* position: fixed;
    left: 0;
    right: 0; */
    bottom: 0;
    height: 90rpx;
    background: #fff;
    box-shadow: 0 -1px 10px 0 rgba(0, 0, 0, 0.10);
    display: flex;
    align-items: center;
    z-index: 160;
}
.bg-c{
    background: #F5F5F5;
