  width: calc(100% - 72rpx);
}

.note-user-icon{
  width: 60rpx;
  height: 60rpx;
  border-radius: 60rpx;
  margin-right: 12rpx;
}
.note-username{
  font-family: PingFangSC-Regular;
  font-size: 26rpx;
  color: #666666;
  line-height: 26rpx;
  overflow:hidden; 
  text-overflow:ellipsis;
  white-space:nowrap;
  /* max-width: 70%; */
}

.note-time{
  font-family: PingFangSC-Regular;
  font-size: 24rpx;
  color: #BBBBBB;
  text-align: justify;
  line-height: 24rpx;
  display: flex;
  align-items: center;
}

.note-view{
  width: 28rpx;
  height: 28rpx;
  margin-right: 4rpx;
