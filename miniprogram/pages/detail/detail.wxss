.container {
    background: rgb(238, 238, 245);
    padding-bottom: 160rpx;
}

.banner {
    position: relative;
    height: 440rpx;
}

.banner image {
    height: 440rpx;
    width: 100%;
}

.banner>.detail {
    position: absolute;
    display: flex;
    left: 0;
    right: 0;
    bottom: 25rpx;
    padding: 0 40rpx;
    color: #FFF;
}

.banner>.detail view:nth-of-type(1) {
    flex-grow: 1;
    font-size: 30rpx;
}

.banner>.detail view:nth-of-type(2) {
    height: 40rpx;
    line-height: 40rpx;
    padding: 0 20rpx 0 50rpx;
    border-radius: 100px;
    font-size: 24rpx;
    background: url(data:image/png;base64,) 20rpx center / 24rpx 20rpx no-repeat, rgba(0, 0, 0, 0.35);
}

.scenic-detail {
    padding: 30rpx 30rpx 0;
    background: #fff;
}

.scenic-detail .name {
    font-size: 36rpx;
    color: #333;
    font-weight: bold;
}

.scenic-detail .other {
    font-size: 24rpx;
    color: #666;
    margin-top: 2rpx;
    margin-left: -30rpx;
}

.scenic-detail .other view {
    display: inline-block;
}

/* .scenic-detail .other .num1 {
    padding-right: 10rpx;
    border-right: 1rpx solid #999;
} */

.scenic-detail .other view text {
    color: #FF8800;
}

/* .scenic-detail .other .num2 {
    margin-left: 10rpx;
} */

.msg {
    margin-top: 20rpx;
    padding-bottom: 28rpx;
    background: #fff;
}

.msg>.title {
    background: #fff;
    padding: 28rpx 30rpx;
    font-size: 32rpx;
    color: #333;
    font-weight: bold;
    border-bottom: 1rpx solid #E4E4E4;
}

.list {
    padding: 28rpx 30rpx 0;
    background: #fff;
}

.list .title {
    font-size: 28rpx;
    color: #333;
    font-weight: bold;
}

.list .cot {
    font-size: 28rpx;
    color: #666;
    margin-top: 14rpx;
    padding-right: 25rpx;
    word-break: break-all;
}

.tip {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 5;
    -webkit-box-orient: vertical;
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/zhankaiH.png) right bottom / 28rpx 28rpx no-repeat;
}

.tipup {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/shouqiH.png) right bottom / 28rpx 28rpx no-repeat;
}

.list .cot.down {
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 2;
    -webkit-box-orient: vertical;
}

.list .cot.up {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/shouqiH.png) right bottom / 28rpx 28rpx no-repeat;
}

.list .cot.down {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/zhankaiH.png) right bottom / 28rpx 28rpx no-repeat;
}

.list .cot.blue text {
    color: #5CA2F8;
}

.list .cot.address {
    position: relative;
    padding-left: 24rpx;
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/mapYellow.png) left center / 20rpx 27rpx no-repeat;
}

.list .cot.address::after {
    content: "";
    position: absolute;
    right: 6rpx;
    top: 0;
    width: 18rpx;
    height: 18rpx;
    margin-top: 18rpx;
    border-top: 2rpx solid #aaa;
    border-right: 2rpx solid #aaa;
    -webkit-transform: translateY(-50%) rotate(45deg);
    transform: translateY(-50%) rotate(45deg);
}

.cot text {
    font-size: 28rpx;
    color: #333;
}

.cot text:last-child {}

.list.time {
    padding-top: 10rpx;
}

.other-list {
    margin-top: 20rpx;
    background: #fff;
    padding: 0 30rpx;
}

.other-list .title {
    border-bottom: 1rpx solid #E4E4E4;
    margin: 0 -30rpx;
    display: flex;
    align-items: center;
    padding: 30rpx;
}

.other-list .list {
    display: flex;
    padding: 30rpx 0;
    border-bottom: 1rpx solid #E4E4E4;
}

.other-list .list:nth-last-of-type(1) {
    border: 0;
}

.other-list .list .left {
    width: 180rpx;
    height: 120rpx;
    background: #eee;
}

.other-list .list .left image {
    width: 180rpx;
    height: 120rpx;
    border-radius: 4px;
}

.other-list .list .right {
    padding-left: 20rpx;
    flex: 1;
    overflow: hidden;
}

.other-list .title .name {
    flex-grow: 1;
    font-size: 32rpx;
    font-weight: bold;
    color: #333;
}

.other-list .title .more {
    font-size: 24rpx;
    padding-right: 25rpx;
    color: #666;
    background: url(data:image/png;base64,) right 7rpx / 15rpx 24rpx no-repeat;
}

.other-list .right .name,
.other-list .right .other,
.other-list .right .other2 {
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
}

.other-list .right .other {
    font-size: 24rpx;
    color: #666;
    margin-top: 17rpx;
}

.other-list .right .other2 {
    font-size: 24rpx;
    color: #666;
    margin-top: 13rpx;
}

.other-list .right .name {
    font-weight: bold;
    line-height: 30rpx;
    font-size: 30rpx;
    color: #333;
}

.add-router {
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 100rpx;
    line-height: 100rpx;
    text-align: center;
    background-image: linear-gradient(-90deg, #FF8800 0%, #FFBD46 100%);
    font-size: 32rpx;
    color: #fff;
}
.added-router{
    position: fixed;
    left: 0;
    right: 0;
    bottom: 0;
    height: 160rpx;
    line-height: 160rpx;
    background: #FFFFFF;
    box-shadow: 0 0 16rpx 0 rgba(0,0,0,0.10);
}
.added-router view{
    width: 690rpx;
    height: 90rpx;
    margin: 35rpx auto;
    line-height: 90rpx;
    text-align: center;
    background-image: linear-gradient(90deg, #FFBD46 0%, #FF8800 100%);
    border-radius: 200rpx;
    font-size: 32rpx;
    color: #fff;
}
.bottom-other {
    background: #fff;
    font-size: 28rpx;
    color: #333;
}

.bottom-other view button {
    padding-right: 40rpx;
    font-size: 28rpx;
}

.bottom-other view button::after {
    border: none;
}

.bottom-other view:nth-of-type(1) button {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/want.png) right center / 32rpx 28rpx no-repeat;
}

.bottom-other view:nth-of-type(1).select button {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/wanted.png) right center / 32rpx 28rpx no-repeat;
    color: #FF8800;
}

.bottom-other view:nth-of-type(2) button {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/quguoH.png) right center / 32rpx 28rpx no-repeat;
}

.bottom-other view:nth-of-type(2).select button {
    background: url(http://pic.lvmama.com/mobile/img/sp/zhiyu/quguoY.png) right center / 32rpx 28rpx no-repeat;
    color: #FF8800;
}

.pop {
    width: 100%;
    height: 100%;
    position: fixed;
    top: 0;
    z-index: 103;
    background-color: rgba(0, 0, 0, .5);
}

.popMsg {
    display: flex;
    width: 66%;
    height: 200rpx;
    flex-direction: column;
    background: #FFFFFF;
    border-radius: 28rpx;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    -webkit-transform: translate(-50%, -50%);
    text-align: center;
}

.butSmg {
    height: 120rpx;
    line-height: 120rpx;
    border-bottom: 1rpx solid #DDDDDD;
    font-size: 36rpx;
    color: #333333;
}

.butSure {
    display: flex;
}

.butSure_N {
    width: 50%;
    border-right: 1rpx solid #DDDDDD;
    font-size: 28rpx;
    line-height: 74rpx;
    color: #999;
}

.butSure_Y {
    width: 50%;
    font-size: 28rpx;
    line-height: 74rpx;
    color: #5CA2F8;
}


.note-title{
  font-family: PingFangSC-Medium;
  font-size: 30rpx;
