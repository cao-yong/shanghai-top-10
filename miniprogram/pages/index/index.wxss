.scenicQuery {
    height: 36rpx;
    /* line-height: 36rpx; */
    vertical-align: middle;
    background: #EEF0F3;
    border-radius: 6rpx;
    color: #5673A6;
    padding: 3rpx 8rpx;
    margin-right: 12rpx;
}

.justifyContent {
    display: flex;
    display: -webkit-flex;
    justify-content: space-between;
    -webkit-justify-content: space-between;
    -moz-justify-content: space-between;
    -ms-justify-content: space-between;
    -o-justify-content: space-between;
}

.overHidden {
    /* overflow: hidden; */
}

.bg-gray {
    /* background: #F9F9F9; */
    width: 100%;
    background-image: linear-gradient(180deg, #FFFFFF 0%, #F9F9F9 57%);
}

.oneLine {
    display: -webkit-box;
    -webkit-line-clamp: 1;
    overflow: hidden;
    text-overflow: ellipsis;
    word-break: break-all;
    -webkit-box-orient: vertical;
}

.localImg {
    width: 220rpx;
    height: 160rpx;
    margin-right: 20rpx;
    border-radius: 8rpx;
}

.scenicLocation {
    height: 60rpx;
    border: 1rpx solid #DDDDDD;
    border-radius: 30rpx;
    line-height: 60rpx;
}

.location {
    display: inline-block;
    height: 60rpx;
    line-height: 60rpx;
    padding-right: 40rpx;
}

.location::after {
    content: "";
    position: absolute;
    border-top: 3rpx solid #999999;
    border-left: 3rpx solid #999999;
    width: 12rpx;
    height: 12rpx;
    transform: rotate(132deg);
    top: 25rpx;
    margin-left: 10rpx;
}

.addressSite {
    width: 28rpx;
    height: 32rpx;
    margin-top: 15rpx;
    padding-left: 20rpx;
    padding-right: 6rpx;
    float: left;
    /* position: absolute; */
}

.scenic {
    width: calc((100% - 20rpx)/2);
    height: 100%;
    border-radius: 16rpx;
    box-shadow: 0 4rpx 20rpx 3rpx rgba(0, 0, 0, 0.10);
    margin-bottom: 30rpx;
    padding-bottom: 24rpx;
    float: left;
    background: #ffffff;
}

.scenic:nth-child(2n+1) {
    margin-right: 20rpx;
}

.scenic image {
    width: 100%;
    height: 220rpx;
    border-top-left-radius: 16rpx;
    border-top-right-radius: 16rpx;
}

.footer {
    position: fixed;
    bottom: 0;
    height: 100rpx;
    font-size: 22rpx;
    width: 100%;
    background: #ffffff;
}

.footerItem {
    width: 33%;
    float: left;
    text-align: center;
}

.footerIcon {
    width: 48rpx;
    height: 52rpx;
    margin-top: 12rpx;
}

.displayBlock {
    display: block;
}

.verticalOne {
    display: -webkit-box;
    overflow: hidden;
    text-overflow: ellipsis;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
}